{
    "buildCommand": "cd frontend && pnpm install && pnpm build",
    "outputDirectory": "frontend/.next",
    "framework": "nextjs",
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "https://snel-pointless.vercel.app/api/:path*"
        }
    ],
    "env": {
        "NEXT_PUBLIC_API_URL": "https://snel-pointless.vercel.app"
    }
}