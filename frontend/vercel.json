{
    "buildCommand": "cd frontend && pnpm install && pnpm build",
    "outputDirectory": "frontend/.next",
    "framework": "nextjs",
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "/api/:path*"
        }
    ],
    "env": {
        "NEXT_PUBLIC_API_URL": ""
    }
}