{
    "version": 2,
    "builds": [
        {
            "src": "api.py",
            "use": "@vercel/python",
            "config": {
                "maxDuration": 60,
                "memory": 1024,
                "runtime": "python3.12",
                "handler": "app",
                "includeFiles": [
                    "kyber.py",
                    "configure_logging.py",
                    "requirements.txt",
                    "app/**/*",
                    ".env*"
                ]
            }
        },
        {
            "src": "frontend/package.json",
            "use": "@vercel/next"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api.py/$1",
            "headers": {
                "Access-Control-Allow-Origin": "https://snel-pointless.vercel.app",
                "Access-Control-Allow-Methods": "GET,POST,PUT,DELETE,OPTIONS",
                "Access-Control-Allow-Headers": "X-CSRF-Token, X-Requested-With, Accept, Accept-Version, Content-Length, Content-MD5, Content-Type, Date, X-Api-Version, X-OpenAI-Key"
            }
        },
        {
            "src": "/((?!api/).*)",
            "dest": "/frontend/$1"
        }
    ],
    "regions": [
        "iad1"
    ],
    "env": {
        "PYTHONUNBUFFERED": "1",
        "PYTHONPATH": ".",
        "PYTHONIOENCODING": "utf-8",
        "QUICKNODE_ENDPOINT": "https://api.kyberswap.com",
        "MORALIS_API_KEY": "${MORALIS_API_KEY}",
        "REDIS_URL": "${REDIS_URL}"
    }
}