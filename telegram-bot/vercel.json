{
    "version": 2,
    "builds": [
        {
            "src": "src/index.js",
            "use": "@vercel/node",
            "config": {
                "memory": 1024,
                "maxDuration": 10
            }
        },
        {
            "src": "api/**/*.js",
            "use": "@vercel/node",
            "config": {
                "memory": 1024,
                "maxDuration": 10
            }
        }
    ],
    "routes": [
        {
            "src": "/api/webhook",
            "dest": "/api/webhook.js"
        },
        {
            "src": "/api/test",
            "dest": "/api/test.js"
        },
        {
            "src": "/(.*)",
            "dest": "src/index.js"
        }
    ]
}