{
    "version": 2,
    "buildCommand": "npm run build",
    "outputDirectory": "build",
    "builds": [
        {
            "src": "src/index.js",
            "use": "@vercel/node",
            "config": {
                "maxDuration": 60
            }
        }
    ],
    "routes": [
        {
            "src": "/api/webhook",
            "dest": "src/api/webhook.js"
        },
        {
            "src": "/(.*)",
            "dest": "src/index.js"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    }
}