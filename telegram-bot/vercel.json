{
    "version": 2,
    "buildCommand": "npm run build",
    "outputDirectory": "build",
    "builds": [
        {
            "src": "src/index.js",
            "use": "@vercel/node"
        },
        {
            "src": "src/api/webhook.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/webhook",
            "dest": "src/api/webhook.js",
            "methods": [
                "GET",
                "POST"
            ]
        },
        {
            "src": "/(.*)",
            "dest": "src/index.js"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    },
    "functions": {
        "src/api/webhook.js": {
            "memory": 1024,
            "maxDuration": 10
        }
    }
}