# SNEL DeFi Agent - Coral Protocol Configuration
# Copy this file to .env and fill in your values

# Coral Protocol Configuration
CORAL_SSE_URL=wss://coral-protocol.com/agents
CORAL_AGENT_ID=snel-defi-agent
CORAL_ORCHESTRATION_RUNTIME=production

# AI Model Configuration
MODEL_NAME=gpt-4
MODEL_PROVIDER=openai
MODEL_API_KEY=your_openai_api_key_here
MODEL_TEMPERATURE=0.1
MODEL_TOKEN_LIMIT=8000
BASE_URL=

# Alternative: Anthropic Configuration
# MODEL_NAME=claude-3-sonnet-20240229
# MODEL_PROVIDER=anthropic
# MODEL_API_KEY=your_anthropic_api_key_here

# Existing SNEL Configuration (should already be in your main .env)
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Web3 Configuration
WEB3_PROVIDER_URL=your_web3_provider_url
ETHEREUM_PRIVATE_KEY=your_private_key_here

# Brian AI API
BRIAN_API_KEY=your_brian_api_key

# Axelar Configuration  
AXELAR_ENVIRONMENT=testnet
AXELAR_API_URL=https://api.testnet.axelar.network
